package HW8Task3;

import java.io.File;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		File folder=new File(".");
		File []files =folder.listFiles();
		for (int i=0;i<files.length;i++) {
			if (files[i].isDirectory()==true) {
				System.out.println(files[i]);
			}
		}
	}

}
